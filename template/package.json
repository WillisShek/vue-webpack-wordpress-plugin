{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "1.0.0",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": ["src", "test"],
      "extensions": "js,jsx,scss,vue"
    }
  },
  "dependencies": {
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.9.0",
    "npm-watch": "^0.2.0",
    {{#if_eq preprocessor "LESS"}}
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    {{/if_eq}}
    {{#if_eq preprocessor "Sass"}}
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    {{/if_eq}}
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0", 
    {{#webpack-dashboard}}
    "webpack-dashboard": "^0.4.0",
    {{/webpack-dashboard}}
    "webpack-dev-server": "^2.2.0"
  }
}